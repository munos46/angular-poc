{
  "name": "sbng",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.config.json",
    "build": "ng build",
    "sb_components_build": "ng build sb-components && npm run sb_components_build_styles && npm run sb_components_copy_assets",
    "//": "If scss needs bundle (imports) add this step and dependency : && scss-bundle -e ./projects/sb-components/src/lib/style/_style.scss -d ./dist/sb-components/style/_styles.scss",
    "sb_components_build_styles": "cpx \"./projects/sb-components/src/lib/style/**/*\" \"./dist/sb-components/style\"",
    "sb_components_copy_assets": "cpx \"./src/assets/**/*\" \"./dist/sb-components/assets\"",
    "sb_components_package": "npm run sb_components_build && cd dist/sb-components && npm pack",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.7",
    "@angular/cdk": "^7.3.3",
    "@angular/common": "^7.2.7",
    "@angular/compiler": "^7.2.7",
    "@angular/core": "^7.2.7",
    "@angular/forms": "^7.2.7",
    "@angular/http": "^7.2.7",
    "@angular/material": "^7.3.3",
    "@angular/platform-browser": "^7.2.7",
    "@angular/platform-browser-dynamic": "^7.2.7",
    "@angular/router": "^7.2.7",
    "bootstrap": "^4.3.1",
    "core-js": "^2.6.5",
    "hammerjs": "^2.0.8",
    "jquery": "^3.4.0",
    "ng": "0.0.0",
    "popper.js": "^1.14.7",
    "primeicons": "^1.0.0",
    "primeng": "^7.0.5",
    "prismjs": "^1.16.0",
    "rxjs": "~6.4.0",
    "serve": "^11.0.0",
    "typescript": "^3.2.4",
    "zone.js": "^0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.13.8",
    "@angular-devkit/build-ng-packagr": "^0.13.8",
    "@angular/cli": "^7.3.8",
    "@angular/compiler-cli": "^7.2.7",
    "@angular/language-service": "^7.2.7",
    "@types/jasmine": "^3.3.9",
    "@types/jasminewd2": "^2.0.6",
    "@types/node": "^11.9.5",
    "codelyzer": "~4.5.0",
    "cpx": "^1.5.0",
    "jasmine-core": "^3.3.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.5",
    "karma-jasmine": "^2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "ng-packagr": "^5.1.0",
    "protractor": "^5.4.2",
    "ts-node": "~8.0.2",
    "tsickle": "^0.34.3",
    "tslib": "^1.9.0",
    "tslint": "^5.13.0"
  }
}
